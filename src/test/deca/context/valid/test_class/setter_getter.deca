class SimpleClass {
    int number;

    void setNumber(int n) {
        number = n;
    }

    int getNumber() {
        return number;
    }
}

{
    SimpleClass obj = new SimpleClass();
    obj.setNumber(42);
    println(obj.getNumber());
    
}
