class A {
    int sum(int a, int b) {
        return a + b;
    }

    int sum(int a, int b, int c) {
        return a + b + c;
    }
}

{
    A a = new A();
    println(a.sum(10, 20) + " " + a.sum(10, 20, 20));
}
