class Rectangle {
        int width;
        int height;
        
        
        void rectangle(int w, int h) {
            this.width = w;
            this.height = h;
        }

        
        int area() {
            return this.width * this.height;
        }

        boolean isLargerThan(Rectangle other) {
            return this.area() > other.area();
        }
    }


{

    // Créer deux instances de Rectangle
    Rectangle r1 = new Rectangle();
    Rectangle r2 = new Rectangle();
    r1.rectangle(2,6);
    r2.rectangle(8,6);
    
    println(r1.area());        
    println(r2.area());        
    
}
