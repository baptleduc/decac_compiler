class TestAsm {
    
    int x;
    int addAsm(int a, int b) {
        methodAsm (
            "LOAD R1, #a",
            "ADD R1, #b",
            "STORE R1, x"
        );
        return x;
    }
}

{
    TestAsm obj = new TestAsm();

    int result;
    result = obj.addAsm(4, 6);

    print("Result: ", result);
}
