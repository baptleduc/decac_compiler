// Description:
//    Testing a children class inherited from a parent class and redefines a method.
//
// Results:
//    Some soundBark
//
// History:
//    Created on 6/1/2025

class Animal {
        void makeSound() {
            print("Some sound");
        }
    }

    class Dog extends Animal {
        void makeSound() {
            print("Bark");
        }
    }

{

    Animal a = new Animal();
    
    Dog d = new Dog();

    a.makeSound();
    d.makeSound();
}
