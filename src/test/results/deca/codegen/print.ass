; start main program
	TSTO #0	; 0 (variables) + 0 (saved registers) + 0 (temporaries) + 0 (method parameters x 2)
	BOV stack_overflow_error_0
	ADDSP #1
; Main program
; Beginning of main instructions:
	LOAD #1, R1
	WINT
	LOAD #0, R1
	WINT
stack_overflow_error_0:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
	HALT
; end main program
