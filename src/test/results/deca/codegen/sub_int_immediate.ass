; start main program
	TSTO #1	; 1 (variables) + 0 (saved registers) + 0 (temporaries) + 0 (method parameters x 2)
	BOV stack_overflow_error
	ADDSP #1
; Main program
; Beginning of main instructions:
	LOAD #11, R2
	SUB #1, R2
	STORE R2, 2(GB)
	LOAD 2(GB), R1
	WINT
	WNL
stack_overflow_error:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
	HALT
; end main program
