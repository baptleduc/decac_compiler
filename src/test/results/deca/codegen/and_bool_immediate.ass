; start main program
	TSTO #1	; 1 (variables) + 0 (saved registers) + 0 (temporaries) + 0 (method parameters x 2)
	BOV stack_overflow_error_2
	ADDSP #1
; Main program
; Beginning of main instructions:
	LOAD #1, R2
	CMP #0, R2
	BEQ set_false_0	; If R2 is false, branch to set_false_0
	LOAD #0, R3
	CMP #0, R3
	BEQ set_false_0	; If R3 is false, branch to set_false_0
	LOAD #1, R2
	BRA end_1
set_false_0:
	LOAD #0, R2
end_1:
	STORE R2, 2(GB)
stack_overflow_error_2:
	WSTR "Error: Stack Overflow"
	WNL
	ERROR
	HALT
; end main program
